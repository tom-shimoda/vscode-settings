{
  "editor.fontLigatures": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#3c3836",
      "type": "whole-line",
      "iconColour": "green",
      "background": "#d79921"
    },
    "FIXME": {
      "foreground": "#3c3836",
      "type": "whole-line",
      "iconColour": "red",
      "background": "#b16286"
    }
  },
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#383838",
    "editor.wordHighlightBackground": "#ff6e007c",
    "editor.selectionBackground": "#004444"
  },
  "terminal.integrated.tabs.enabled": true,
  "editor.foldingMaximumRegions": 65000,
  "vscode-neovim.neovimInitVimPaths.darwin": "~/vscode_neovim/init.vim",
  "editor.minimap.renderCharacters": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.views.commits.files.layout": "list",
  "codetour.promptForWorkspaceTours": false,
  "workbench.editor.wrapTabs": true,
  "blender.executables": [
    {
      "path": "/Applications/Blender.app/Contents/MacOS/blender",
      "name": "",
      "isDebug": false
    }
  ],
  "editor.inlineHints.fontFamily": "'Delugia Nerd Font', Consolas, 'Courier New', monospace",
  "gitlens.codeLens.scopes": [
    "document",
    "containers",
    "blocks"
  ],
  "bracketLens.mode": "auto",
  "editor.minimap.showSlider": "always",
  "editor.fontSize": 11,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "bracketLens.minBracketScopeLines": 30,
  "editor.bracketPairColorization.enabled": true,
  "editor.minimap.maxColumn": 50,
  "sourcetrail.startServerAtStartup": true,
  "C_Cpp.updateChannel": "Insiders",
  "editor.minimap.size": "fill",
  "editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
  "fzf-quick-open.findDirectoriesCmd": "fd --type d",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.win32": "C:\\vscode_neovim\\init.vim",
  "editor.accessibilitySupport": "off",
  "editor.renderWhitespace": "boundary",
  "extensions.ignoreRecommendations": true,
  "csharp.suppressDotnetInstallWarning": true,
  "workbench.startupEditor": "none",
  "files.exclude": {
    "**/*.meta": true
  },
  "settingsSync.ignoredSettings": [
    "editor.fontSize"
  ],
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\tools\\neovim\\nvim-win64\\bin\\nvim.exe",
  "terminal.integrated.fontSize": 11,
  "omnisharp.useGlobalMono": "always",
  "codetour.showMarkers": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "zenMode.hideTabs": false,
  "files.autoSave": "afterDelay",
  "liveshare.presence": false,
  "liveshare.connectionMode": "relay",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.emptyWindow": false,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true
}